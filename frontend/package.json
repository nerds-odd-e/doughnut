{
  "name": "doughnut-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=24",
    "pnpm": ">=10.26.2"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "syncpack fix-mismatches",
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check . && vue-tsc --noEmit",
    "format": "biome check --write . && vue-tsc --noEmit",
    "storybook": "storybook dev -p 6006 --no-open"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "marked": "^17.0.1",
    "mini-debounce": "^1.0.8",
    "quill": "2.0.3",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2",
    "vue-content-loader": "2.0.1",
    "vue-router": "4.6.4",
    "vue-toastification": "2.0.0-rc.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@storybook/addon-essentials": "8.6.15",
    "@storybook/addon-interactions": "8.6.15",
    "@storybook/addon-onboarding": "8.6.15",
    "@storybook/blocks": "8.6.15",
    "@storybook/test": "8.6.15",
    "@storybook/vue3": "8.6.15",
    "@storybook/vue3-vite": "8.6.15",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "25.0.3",
    "@types/turndown": "^5.0.6",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/server-renderer": "^3.5.26",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "^10.4.23",
    "daisyui": "4.12.23",
    "es-toolkit": "^1.43.0",
    "esbuild": "0.27.2",
    "gsap": "^3.14.2",
    "jsdom": "^27.3.0",
    "optionator": "^0.9.4",
    "postcss": "8.5.6",
    "postcss-load-config": "6.0.1",
    "sass": "1.97.1",
    "storybook": "8.6.15",
    "tailwindcss": "3.4.19",
    "terser": "^5.44.1",
    "typescript": "5.9.3",
    "unimport": "^5.6.0",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-inspector": "^2.3.2",
    "unplugin-vue-router": "^0.19.1",
    "vite": "7.3.0",
    "vite-plugin-checker": "^0.12.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "4.0.16",
    "vitest-dom": "^0.1.1",
    "vitest-fetch-mock": "^0.4.5",
    "vue": "3.5.26",
    "vue-tsc": "3.2.1"
  }
}
