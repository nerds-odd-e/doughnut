{
  "name": "doughnut-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=23",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "syncpack fix-mismatches",
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "format": "biome check --write ."
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "gsap": "^3.12.7",
    "intersection-observer": "^0.12.2",
    "marked": "^15.0.6",
    "mini-debounce": "^1.0.8",
    "quill": "2.0.3",
    "turndown": "^7.2.0",
    "vue": "3.5.13",
    "vue-content-loader": "2.0.1",
    "vue-router": "4.5.0",
    "vue-toastification": "2.0.0-rc.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.12.0",
    "@types/turndown": "^5.0.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "es-toolkit": "^1.32.0",
    "jsdom": "^26.0.0",
    "optionator": "^0.9.4",
    "postcss": "^8.5.1",
    "postcss-load-config": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "unplugin-vue-inspector": "^2.3.1",
    "unplugin-vue-router": "^0.11.2",
    "vite": "6.0.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "3.0.4",
    "vitest-dom": "^0.1.1",
    "vitest-fetch-mock": "^0.4.3",
    "vue-tsc": "2.2.0"
  }
}
